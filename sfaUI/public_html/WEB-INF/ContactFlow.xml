<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ContactFlow">
    <default-activity>searchcontact</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="searchcontact">
      <page>/ContactSearch.jsff</page>
    </view>
    <view id="contactcreate">
      <page>/ContactForm.jsff</page>
    </view>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>Rollback</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Commit">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>Commit</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Commit1">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>Commit</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Execute">
      <method>#{bindings.Execute.execute}</method>
      <outcome>
        <fixed-outcome>Execute</fixed-outcome>
      </outcome>
    </method-call>
    <view id="ContactDetails">
      <page>/ContactDetails.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>searchcontact</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>contactCreate</from-outcome>
        <to-activity-id>contactcreate</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__15">
        <from-outcome>delete</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>contactDetails</from-outcome>
        <to-activity-id>ContactDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>contactcreate</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__13">
        <from-outcome>submit</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>submitContinue</from-outcome>
        <to-activity-id>Commit1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__12">
        <to-activity-id>searchcontact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>Commit1</from-activity-id>
      <control-flow-case id="__18">
        <to-activity-id>contactcreate</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>Commit</from-activity-id>
      <control-flow-case id="__9">
        <to-activity-id>Execute</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>Execute</from-activity-id>
      <control-flow-case id="__14">
        <to-activity-id>searchcontact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>ContactDetails</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>cancelDetails</from-outcome>
        <to-activity-id>searchcontact</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
